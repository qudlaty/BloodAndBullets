{
  "extends": ["airbnb-base", "airbnb/rules/react", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/button-has-type": "off",
    "react/jsx-filename-extension": "off",
    "import/no-extraneous-dependencies": "off"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
